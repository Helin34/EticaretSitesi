@model List<Alina_eticaretsitesi.Data.Models.Dress>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>Dress List</h3>
<br />
<table class=" table table-bordered">
    <tr>
        <th>ID</th>
        <th>Dress Name</th>
        <th>Category Name</th>
        <th>Delete</th>
        <th>Update</th>
        <th>Details</th>
    </tr>
    @foreach (var x in Model)
    {
<tr>

    <td>@x.DressID</td>
    <td>@x.DressName  </td>
    <td></td>
    <td><a href="#" class=" btn btn-danger">Delete</a></td>
    <td><a href="#" class=" btn btn-success">Update</a></td>
    <td><a href="#" class=" btn btn-info">Details</a></td>

</tr>
    }
</table>
<a href="#" class=" btn btn-primary">New Dress</a>



